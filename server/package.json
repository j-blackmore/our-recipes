    "coverage:test": "jest --coverage",
    "watch:test": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "mongoose": "^5.9.10"
  },
  "engines": {
    "node": "10.13.0"
  },
  "devDependencies": {
    "jest-cli": "^25.4.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "./client"
